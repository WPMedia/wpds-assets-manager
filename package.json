{
  "name": "@washingtonpost/wpds-assets",
  "version": "1.1.3",
  "description": "",
  "targets": {
    "./": {
      "isLibrary": true,
      "includeNodeModules": {
        "react": false
      },
      "outputFormat": "commonjs",
      "distDir": "asset"
    }
  },
  "files": [
    "asset",
    "README.md"
  ],
  "author": "artmsilva <artmsilva@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-private-methods": "^7.16.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.0",
    "@babel/plugin-transform-typescript": "^7.16.1",
    "@parcel/babel-plugin-transform-runtime": "^2.0.1",
    "@parcel/packager-ts": "^2.0.1",
    "@parcel/transformer-typescript-types": "^2.0.1",
    "@svgr/cli": "^6.1.1",
    "@types/react": "^17.0.37",
    "parcel": "^2.0.1",
    "pascal-case": "^3.1.2",
    "react": "^16.0.1 || ^17.0.2",
    "react-dom": "^16.0.1 || ^17.0.2",
    "semantic-release": "^18.0.1",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react": "^16.0.1 || ^17.0.2",
    "react-dom": "^16.0.1 || ^17.0.2"
  },
  "scripts": {
    "dev": "make dev",
    "build": "make build",
    "semantic-release": "semantic-release"
  },
  "prettier": {},
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@washingtonpost/wpds-ui-kit": "0.1.0-experimental.12",
    "ac-colors": "^1.4.3",
    "next": "latest",
    "react": "^16.0.1 || ^17.0.2",
    "react-dom": "^16.0.1 || ^17.0.2",
    "rgb-hex": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WPMedia/wpds-assets-manager.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
